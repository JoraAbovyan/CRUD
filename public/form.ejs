<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>
<body class="body">

    <ul>
        <% obj.forEach(function(justOne) { %>
        <li>
        Id - <strong><%= justOne.theaterId %></strong> <br>
        City - <strong><%= justOne.location.address.city %></strong> /
        State - <strong><%= justOne.location.address.state %></strong> /
        Zip code - <strong><%= justOne.location.address.zipcode %></strong> /
        Street - <strong><%= justOne.location.address.street1 %></strong> <br> <br>
        </li>
        <% }); %>
        </ul>

    <div>
            <img src="download.jpeg" alt="">
    </div>
    <div>
        <!-- <form class="form" action="/about" method="post">  -->
            <input id="usName" type="text" required name="name" placeholder="Enter your name"><br>
        <input  id="usEmail"  type="email" name="email" placeholder="Enter your email"><br>
        <input  id="usPassword" type="password" name="password" placeholder="Enter your password"><br>
        <input type="submit" onclick="sendData()" class="submit">
            <!-- </form> -->
    </div>
   

    <script src="script.js"></script>
</body>
</html>